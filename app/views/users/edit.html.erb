<h1>Edit a User</h1>

<%= form_for @user do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <br>
    <br>
    
    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.label :img_url %>
    <%= f.text_field :img_url%>

    <br>
    <br>
    
    <%= f.submit %>

<% end %>