<div class="welcome_page_wrapper">
    <div class="welcome_image"> 
        <%= image_tag("checklist-logo.png", :alt => "image") %>
    </div>
    <div class="logged_in_wrapper">
        <div class="logged_in_question">
        <% if logged_in? %>
            <h1>You are Logged In, <%= current_user.username.capitalize %></h1>
        <%else%>
         <h1>czechoslovakian czecklist </h1>
        <div class="left"> <%= button_to "Sign Up", '/users/new', method: :get%></div>

        <%end%>
        </div>
    </div>
</div>